    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "J = 1\n",
    "lamb = 5\n",
    "k = np.arange(0,1,0.001)\n",
    "E_1 = 2*J*np.cos(2*np.pi*k) + lamb\n",
    "E_2 = 2*J*np.cos(2*np.pi*k) - lamb\n",
    "plt.plot(k, E_1)\n",
    "plt.plot(k, E_2)\n",
    "plt.xlabel('sample(n)')\n",
    "plt.ylabel('voltage(V)')\n",
